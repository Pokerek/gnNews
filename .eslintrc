{
   "root": true,
   "parser": "@typescript-eslint/parser",
   "plugins": [
      "@typescript-eslint",
      "prettier",
      "formatjs"
   ],
   "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
   ],
   "rules": {
      "no-console": "warn",
      "prettier/prettier": "error",
      "no-empty-function": [
         "error",
         {
            "allow": [
               "methods"
            ]
         }
      ],
      "@typescript-eslint/no-empty-function": "off",
      "formatjs/no-offset": "error"
   }
}